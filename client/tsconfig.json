{
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "target": "ES6",
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "noEmit": false,
    "noFallthroughCasesInSwitch": true,
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "allowJs": true,
    "baseUrl": "src",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "lib": [
      "ESNext",
      "dom",
      "dom.iterable"
    ],
    "sourceMap": true,
    "noImplicitAny": false
  },
  "exclude": [
    "node_modules",
    "build"
  ],
  "include": [
    "src",
    "__mocks__/Data",
    "__mocks__",
    "jest.config.js"
  ]
}